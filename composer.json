{
    "name": "mattybaer/ebay-sdk-php",
    "description": "An eBay SDK for PHP with support for PHP 8. Maintained by Bibliopolis.",
    "keywords": ["ebay", "api", "sdk"],
    "homepage": "https://www.bibliopolis.com/",
    "type": "library",
    "license": "(Apache-2.0 and MIT)",
    "authors": [
        {
            "name": "David T. Sadler",
            "email": "davidtsadler@googlemail.com",
            "homepage": "http://davidtsadler.com"
        },
        {
            "name": "Matthew Baer",
            "email": "mbaer@bibliopolis.com",
            "homepage": "http://www.bibliopolis.com"
        }
    ],
    "support": {
        "issues": "https://github.com/mattybaer/ebay-sdk-php/issues"
    },
    "require": {
        "php-64bit": "^7.2 || ^8.0",
        "ext-curl": "*",
        "ext-libxml": "*",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "guzzlehttp/promises": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "DTS\\eBaySDK\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "DTS\\eBaySDK\\Test\\": "test/"
        },
        "classmap": ["build/"]
    }
}